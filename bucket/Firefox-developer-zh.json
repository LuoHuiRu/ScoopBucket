{
    "version": "139.0b7",
    "description": "Developer builds of Firefox: the popular open source web browser.",
    "homepage": "https://www.mozilla.org/zh-CN/firefox/developer/",
    "license": "MPL-2.0",
    "architecture": {
        "64bit": {
            "url": "https://archive.mozilla.org/pub/devedition/releases/139.0b7/win64/zh-CN/Firefox%20Setup%20139.0b7.exe#/dl.7z",
            "hash": "sha512:dd9459001e3cace7d31e777c841b4947e6721b5aaa92cb4ef1c8e6c91ffb10db70352eda7f48a9ce665a1ee4bff5462e073f5e6380ab657ea0568a7241ece37a"
        }
    },
    "extract_dir": "core",
    "post_install": [
        "firefox-dev -CreateProfile \"Scoop-Dev $persist_dir\\profile\"",
        "$Addition = \"$persist_dir\\addition\"",
        "if (Test-Path $Addition) {",
        "  info 'Copying additional items...'",
        "  Copy-Item -Path $Addition\\* -Destination $dir -Force -Recurse -ErrorAction SilentlyContinue",
        "}",
        "$Police = \"$persist_dir\\distribution\"",
        "if (!(Test-Path $Police)) {",
        "  info 'Creating Police Directory...'",
        "  New-Item -Path $Police -Type Directory -ErrorAction SilentlyContinue",
        "}"
    ],
    "bin": [
        [
            "firefox.exe",
            "firefox-dev"
        ]
    ],
    "shortcuts": [
        [
            "firefox.exe",
            "Firefox Developer Edition"
        ]
    ],
    "persist": [
        "profile",
        "addition",
        "distribution"
    ],
    "checkver": {
        "url": "https://aus5.mozilla.org/update/6/Firefox/93.0/_/WINNT_x86_64-msvc-x64/zh-CN/aurora/_/_/_/_/update.xml",
        "xpath": "/updates/update/patch/@URL",
        "regex": "devedition-([\\db.]+)-"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://archive.mozilla.org/pub/devedition/releases/$version/win64/zh-CN/Firefox%20Setup%20$version.exe#/dl.7z",
                "hash": {
                    "url": "https://archive.mozilla.org/pub/devedition/releases/$version/SHA512SUMS",
                    "regex": "$sha512\\s+win64/zh-CN/$basename"
                }
            }
        }
    }
}
