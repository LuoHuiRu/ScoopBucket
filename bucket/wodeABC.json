    "license": "武汉市核烁科技有限公司",
    "note": [
        "需配置scoop config",
        "\"private_hosts\": [",
        "  {",
        "    \"match\": \"https://files.domain.com/*\",",
        "    \"headers\": \"Referer=https://domain.com/\"",
        "  }",
        "]"
    ],
    "architecture": {
        "64bit": {
            "url": "https://cdn2.wodeabc.com/file/upload/600134/files/update/x64/wodeabc-installer-64bit-6_62_1.exe#/dl.7z",
            "hash": "sha512:64c42e3a33ccac9f88b5130e74130c03b6cb4d0561dc34078f464d725c2510b573e4bbcc4436078b3fb5ce4b2a4912d03cb1d69d8dbd7f664d0c4adb0a8fde36"
        }
    },
    "pre_install": [
        "Expand-7zipArchive \"$dir\\`$PLUGINSDIR\\app-64.7z\" \"$dir\"",
        "Remove-Item \"$dir\\`$PLUGINSDIR\" -Recurse -Force"
    ],
    "bin": "wodeabc.exe",
    "shortcuts": [
        [
            "wodeabc.exe",
            "ABC工具箱"
        ]
    ],
    "checkver": {
        "url": "https://cdn2.wodeabc.com/file/upload/600134/files/update/x64/latest.yml",
        "regex": "version: ([\\d.]+)"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://cdn2.wodeabc.com/file/upload/600134/files/update/x64/wodeabc-installer-64bit-$underscoreVersion.exe#/dl.7z",
                "hash": {
                    "url": "$baseurl/latest.yml",
                    "regex": "sha512:\\s+$base64"
                }
            }
        }
    }
}
