    "note": [
        "需配置scoop config",
        "\"private_hosts\": [",
        "  {",
        "    \"match\": \"https://files.domain.com/*\",",
        "    \"headers\": \"Referer=https://domain.com/\"",
        "  }",
        "]"
    ],
    "architecture": {
        "64bit": {
            "url": "https://cdn2.wodeabc.com/file/upload/600134/files/update/x64/wodeabc-installer-64bit-6_63_0.exe#/dl.7z",
            "hash": "sha512:a1a4f71e00ef58c39a878c8c20b06574f86add7d18aebb4369d26db0283d54cb743cd44cb6aa838a9b62645c23c37a94125eae91f916d822c1d179400478bc55"
        }
    },
    "pre_install": [
        "Expand-7zipArchive \"$dir\\`$PLUGINSDIR\\app-64.7z\" \"$dir\"",
        "Remove-Item \"$dir\\`$PLUGINSDIR\" -Recurse -Force"
    ],
    "bin": "wodeabc.exe",
    "shortcuts": [
        [
            "wodeabc.exe",
            "ABC工具箱"
        ]
    ],
    "checkver": {
        "url": "https://cdn2.wodeabc.com/file/upload/600134/files/update/x64/latest.yml",
        "regex": "version: ([\\d.]+)"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://cdn2.wodeabc.com/file/upload/600134/files/update/x64/wodeabc-installer-64bit-$underscoreVersion.exe#/dl.7z",
                "hash": {
                    "url": "$baseurl/latest.yml",
                    "regex": "sha512:\\s+$base64"
                }
            }
        }
    }
