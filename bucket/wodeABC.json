    "note": [
        "需配置scoop config",
        "\"private_hosts\": [",
        "  {",
        "    \"match\": \"https://files.domain.com/*\",",
        "    \"headers\": \"Referer=https://domain.com/\"",
        "  }",
        "]"
    ],
    "architecture": {
        "64bit": {
            "url": "https://cdn2.wodeabc.com/file/upload/600134/files/update/x64/wodeabc-installer-64bit-6_63_1.exe#/dl.7z",
            "hash": "sha512:8b87e7c3871a09f88be6cba52a31549a90ba14d4f2114bdbe4b342f26ef3a5e4719e83cca5897a9c30c3e7b7008a9b3c692b21f5bade0221d83d587758de4867"
        }
    },
    "pre_install": [
        "Expand-7zipArchive \"$dir\\`$PLUGINSDIR\\app-64.7z\" \"$dir\"",
        "Remove-Item \"$dir\\`$PLUGINSDIR\" -Recurse -Force"
    ],
    "bin": "wodeabc.exe",
    "shortcuts": [
        [
            "wodeabc.exe",
            "ABC工具箱"
        ]
    ],
    "checkver": {
        "url": "https://cdn2.wodeabc.com/file/upload/600134/files/update/x64/latest.yml",
        "regex": "version: ([\\d.]+)"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://cdn2.wodeabc.com/file/upload/600134/files/update/x64/wodeabc-installer-64bit-$underscoreVersion.exe#/dl.7z",
                "hash": {
                    "url": "$baseurl/latest.yml",
                    "regex": "sha512:\\s+$base64"
                }
            }
        }
    }
